    "paths": {
      "/api/{table}": {
        "get": {
          "tags": [
            "api"
          ],
          "summary": "get all objects",
          "description": "Get all objects from csv table",
          "operationId": "getcsv_json",
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "name": "table",
              "in": "path",
              "description": "Table name of the database from which object to fetch",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "400": {
              "description": "Invalid table value"
            }
          }
        },
        "delete": {
          "tags": [
            "api"
          ],
          "summary": "delete objects",
          "description": "Get all objects from csv table",
          "operationId": "delete object",
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "name": "table",
              "in": "path",
              "description": "Table name of the database from which object to fetch",
              "required": true,
              "type": "string"
            },
            {
              "in": "body",
              "name": "body",
              "description": "Pet object that needs to be added to the store",
              "required": true,
              "schema": {
                "$ref": "#/definitions/delete_id"
              }
            }
          ],
          "responses": {
            "400": {
              "description": "Invalid table value"
            }
          }
        },
        "post": {
          "tags": [
            "api"
          ],
          "summary": "objects",
          "description": "Get all objects from csv table",
          "operationId": "post object",
          "consumes": [
            "application/json"
          ],
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "name": "table",
              "in": "path",
              "description": "Table name of the database from which object to fetch",
              "required": true,
              "type": "string"
            },
            {
              "in": "body",
              "name": "body",
              "description": "Pet object that needs to be added to the store",
              "required": true,
              "schema": {
                "$ref": "#/definitions/object_info"
              }
            }
          ],
          "responses": {
            "400": {
              "description": "Invalid table value"
            }
          }
        },
        "put": {
          "tags": [
            "api"
          ],
          "summary": "objects",
          "description": "Get all objects from csv table",
          "operationId": "put object",
          "consumes": [
            "application/json"
          ],
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "name": "table",
              "in": "path",
              "description": "Table name of the database from which object to fetch",
              "required": true,
              "type": "string"
            },
            {
              "in": "body",
              "name": "body",
              "description": "Pet object that needs to be added to the store",
              "required": true,
              "schema": {
                "$ref": "#/definitions/object_info"
              }
            }
          ],
          "responses": {
            "400": {
              "description": "Invalid table value"
            }
          }
        }
      },
      "/api/{table}/{objectID}": {
        "patch": {
          "tags": [
            "api"
          ],
          "summary": "delete objects",
          "description": "Get all objects from csv table",
          "operationId": "update object id ",
          "consumes": [
            "multipart/form-data"
          ],
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "name": "table",
              "in": "path",
              "description": "Table name of the database from which object to fetch",
              "required": true,
              "type": "string"
            },
            {
              "name": "objectID",
              "in": "path",
              "description": "Updated name of the pet",
              "required": true,
              "type": "integer",
              "format": "int64"
            },
            {
              "name": "accessionYear",
              "in": "formData",
              "description": "Updated name of the pet",
              "required": false,
              "type": "integer",
              "format": "int64"
            },
            {
              "name": "primaryImage",
              "in": "formData",
              "description": "Updated status of the pet",
              "required": false,
              "type": "string"
            }
          ],
          "responses": {
            "400": {
              "description": "Invalid table value"
            }
          }
        }
      }
    },
